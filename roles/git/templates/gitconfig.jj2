[user]
	email = {{ email }}
	name = Peter Kuehne
[color]
	ui = true
[alias]
	logdog = log --decorate --oneline --graph
	logdogs = log --decorate --oneline --graph --simplify-by-decoration
	d = difftool
[core]
	excludesfile = ~/.gitignore_global
	editor = vim
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[push]
	default = current
